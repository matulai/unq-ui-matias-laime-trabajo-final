{
  "arrowParens": "always",
  "semi": true,
  "tabWidth": 2,
  "printWidth": 100,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "prettier.endOfLine": "lf",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@components/(.*)$",
    "^@content/(.*)$",
    "^@layouts/(.*)$",
    "^@pages/(.*)$",
    "^@styles/(.*)$",
    "^@/(.*)$",
    "^.[./].*",
    ""
  ]
}
